{
  "name": "aws-nodejs-kit",
  "version": "1.0.0",
  "description": "Sharing utilities across AWS lambda functions",
  "main": "index.js",
  "typings": "index.d.ts",
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "test:coverage": "npm run test -- --collect-coverage",
    "watch": "tsc -w",
    "build": "tsc"
  },
  "files": [
    "/dist",
    "index.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/andrea-lascola/aws-nodejs-kit"
  },
  "author": "Andrea La Scola",
  "private": false,
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.64",
    "@types/dotenv": "^6.1.1",
    "@types/faker": "^4.1.5",
    "@types/jest": "^24.0.13",
    "@types/jsonwebtoken": "^8.3.4",
    "@types/lodash": "^4.14.165",
    "@types/moment": "^2.13.0",
    "@types/node": "11.13.2",
    "@types/object-hash": "^1.3.4",
    "@types/uuid": "^3.4.4",
    "jest": "^29.3.1",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.4.5",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.803.0",
    "class-validator": "^0.9.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "loglevel": "^1.7.1",
    "loglevel-plugin-prefix": "^0.8.4",
    "moment": "^2.29.1"
  },
  "jest": {
    "verbose": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "roots": [
      "<rootDir>/test",
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "modulePathIgnorePatterns": [
      "__mocks__",
      "examples"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.[t]sx?$"
  },
  "keywords": [
    "aws",
    "typescript",
    "logger",
    "lambda",
    "helpers",
    "utilities",
    "monads",
    "middleware"
  ]
}
